<apex:page controller="BulkInsertController" tabStyle="Contact">
<apex:form >
    <apex:pageBlock >
        <apex:pageBlockSection columns="3" title="Values to enter">
            <apex:inputField value="{!s1.Sname__c}" />
            <apex:inputField value="{!s1.Sclass__c}" />
            <apex:inputField value="{!s1.Semail__c}" />
        </apex:pageBlockSection>
        <apex:pageBlockSection columns="3" title="Values to enter">
            <apex:inputField value="{!s2.Sname__c}" />
            <apex:inputField value="{!s2.Sclass__c}" />
            <apex:inputField value="{!s2.Semail__c}" />
        </apex:pageBlockSection>
        <apex:pageBlockSection columns="3" title="Values to enter">
            <apex:inputField value="{!s3.Sname__c}" />
            <apex:inputField value="{!s3.Sclass__c}" />
            <apex:inputField value="{!s3.Semail__c}" />
        </apex:pageBlockSection>
        <apex:pageBlockButtons >
            <apex:commandButton value="Save" Action="{!Save}" />
        </apex:pageBlockButtons>
    </apex:pageBlock>
</apex:form>
</apex:page>